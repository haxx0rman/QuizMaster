        {
          "id": "97476bff-8d5e-48e5-9933-40251d7a8856",
          "text": "The account value will be 90% of the original equity, no margin call.",
          "is_correct": false,
          "explanation": null
        },
        {
          "id": "ed66ac82-8c4e-4f90-89a7-97368e15b8cc",
          "text": "The account value will be 50% of the original equity, no margin call.",
